<h1>Log In:</h1>
<%= form_for @user, url: login_path do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>

  <%= f.label :password %><br>
  <%= f.password_field :password %><br>

  <br>

  <%= f.submit 'Log In' %>

  <p>Don't have an account?</p>
  <%= link_to "Sign Up", new_user_registration_path %>
<% end %>
