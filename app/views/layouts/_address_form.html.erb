<%= form_for address do |f| %>
  <%= f.label :street1 %>
  <%= f.text_field :street1 %>
  <%= f.label :street2 %>
  <%= f.text_field :street2 %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>
<% end %>
